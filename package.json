    "@types/node-fetch": "2.5.12",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "8.0.1",
    "mocha": "9.1.3",
    "node-fetch": "2.6.5",
