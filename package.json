{
  "name": "mwparser",
  "version": "1.3.3",
  "description": "Parse and manipulate MediaWiki's wikicode.",
  "main": "dist/main.js",
  "files": [
    "dist/**",
    "grammars/**"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --fix --ext .js,.ts",
